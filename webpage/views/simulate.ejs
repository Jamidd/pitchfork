</body>
  <header-component></header-component>

  <main>    
    <h1>FungiPackage Locations simular</h1>
    <div id="mapid"></div>
    <form name="mapForm" action="/simulate" onsubmit="return validateForm()" method="post">
      <label for="direccion">Dirección: </label><br>
      <input type="text" id="direccion" name="direccion"><br>
      <label for="cantidad_escobajo">Cantidad Escobajo (KG/Año): </label><br>
      <input type="text" id="cantidad_escobajo" name="cantidad_escobajo"><br>
      <label for="costo_empaquetamiento">Costo Empaquetamiento ($/KG): </label><br>
      <input type="text" id="costo_empaquetamiento" name="costo_empaquetamiento"><br>
      <input type="submit" value="Submit">
    </form>

    <button onclick="execute()"> Ejecutar Optimizacion </button>

    <p value="<%=address>"></p>
  </main>
  <script src="javascripts/map.js" type="text/javascript"></script>
  <script src="javascripts/mapsearch.js" type="text/javascript"></script>
